	public static IProxy proxy;
	
	@Mod.EventHandler
	private void preInit(FMLPreInitializationEvent event){
		
	}
	
	@Mod.EventHandler
	private void init(FMLInitializationEvent event){
		
	}
	
	@Mod.EventHandler
	public void postInit(FMLPostInitializationEvent event){
		
	}
}
